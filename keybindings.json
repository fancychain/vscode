,
    {
        "key": "'",
        "command": "type",
        "args": { "text": "\"" },
        "when": "editorTextFocus"
    },
    {
        "key": "shift+'",
        "command": "type",
        "args": { "text": "'" },
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+9",
        "command": "editor.action.insertSnippet",
        "args": {
            "name": "wrap in function"
        },
        "when": "editorHasSelection"
    },
    {
        "key": "ctrl+shift+0",
        "command": "editor.action.insertSnippet",
        "args": {
            "name": "remove function"
        },
        "when": "editorHasSelection"
    }
